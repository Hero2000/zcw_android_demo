<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <constant name="struts.devMode" value="true" />

    <package name="default" namespace="/" extends="struts-default">
        <action name="login" class="top.zcwfeng.action.UserAction" method="login"/>
        <action name="postString" class="top.zcwfeng.action.UserAction" method="postString"/>
        <action name="postFile" class="top.zcwfeng.action.UserAction" method="postFile"/>
        <action name="uploadInfo" class="top.zcwfeng.action.UserAction" method="uploadInfo"/>

    </package>


    <!-- Add packages here -->
</struts>